# Server function
tab_0_server <- function(input, output, session) {
output$contents <- renderTable({
if (input$disp == "head") {
return(head(df))
}
else {
return(df)
}
})
fit_arima <- eventReactive(input$predictButton, {
print("Evento de clic en el botÃ³n activado")
showNotification("Ejecutando modelo", duration = 5, type = "message")
# Formatear la columna "Fecha" como objeto de fecha
df$Fecha <- as.Date(df$Fecha, format = "%YM%m")
# Ajustar el modelo ARIMA
arima_model <- arima(df$Serie, order = c(1, 0, 1))
# Obtener las predicciones
forecast_result <- predict(arima_model, n.ahead = 10)
# Imprimir las predicciones
print("Predicciones ARIMA:")
print(forecast_result)
return(forecast_result)
})
output$forecast_result <- renderPrint({
fit_arima()
})
}
